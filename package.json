{
	"name": "fivework",
	"version": "0.0.0",
	"private": true,
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev --parallel",
		"lint": "turbo run lint",
		"game": "pnpm --filter @fivework/game",
		"decor": "pnpm --filter @fivework/decorators",
		"nui": "pnpm --filter @fivework/nui",
		"format": "prettier --write \"**/*.{ts,tsx,md}\""
	},
	"devDependencies": {
		"prettier": "latest",
		"turbo": "latest"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"packageManager": "pnpm@7.14.2"
}
